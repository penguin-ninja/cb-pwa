{
  "name": "cb-pwa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.2",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "build-css": "node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/app/ -o src/app/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/app/ -o src/app/ --watch --recursive",
    "eject": "react-scripts eject"
  }
}
